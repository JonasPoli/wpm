<form method="post" action="{{ path('app_admin_post_delete', {'id': post.id}) }}" onsubmit="return confirm('Quer mesmo apagar este item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ post.id) }}">
    <button class="btn btn-danger text-white ms-3 mt-3">Delete</button>
</form>
